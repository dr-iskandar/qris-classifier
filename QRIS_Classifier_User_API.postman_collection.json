{
	"info": {
		"_postman_id": "qris-classifier-user-api-collection",
		"name": "QRIS Classifier User API",
		"description": "User API collection for QRIS Classifier - includes authentication and classification endpoints for regular users",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.success && response.data.token) {",
									"        pm.environment.set('jwt_token', response.data.token);",
									"        console.log('JWT token saved to environment');",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"userpassword\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/auth/login",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"auth",
								"login"
							]
						}
					},
					"response": []
				},
				{
					"name": "Get Current User Info",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/auth",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"auth"
							]
						}
					},
					"response": []
				},
				{
					"name": "Verify Token",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/auth/verify",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"auth",
								"verify"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "API Key Management",
			"item": [
				{
					"name": "Get Current User's API Key Info",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/api-keys",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"api-keys"
							]
						}
					},
					"response": []
				},
				{
					"name": "Regenerate API Key",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.success && response.data.apiKey) {",
									"        pm.environment.set('api_key', response.data.apiKey);",
									"        console.log('Regenerated API key saved to environment');",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{}"
						},
						"url": {
							"raw": "{{base_url}}/api/api-keys",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"api-keys"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "QRIS Image Classification",
			"item": [
				{
					"name": "Classify Business Type (Single Image)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "X-API-Key",
								"value": "{{api_key}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"images\": {\n    \"image1\": \"data:image/jpeg;base64,\",\n    \"metadata\": {\n      \"requestId\": \"req_{{$timestamp}}\",\n      \"clientVersion\": \"1.0.0\"\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/classify",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"classify"
							]
						}
					},
					"response": []
				},
				{
					"name": "Classify Business Type (Multiple Images)",
