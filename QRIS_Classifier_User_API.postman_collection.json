								"value": "Bearer {{jwt_token}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"images\": {\n    \"image1\": \"data:image/jpeg;base64,\"\n  },\n  \"metadata\": {\n    \"requestId\": \"req_{{$timestamp}}\",\n    \"clientVersion\": \"1.0.0\"\n  }\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/classify",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"classify"
							]
						}
					},
					"response": []
				}
			]
		},
