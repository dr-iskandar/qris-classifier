		{
			"name": "User Management (Admin)",
			"item": [
				{
					"name": "Create New User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    const response = pm.response.json();",
									"    if (response.success && response.data.user.apiKey) {",
									"        pm.environment.set('new_user_api_key', response.data.user.apiKey);",
									"        pm.environment.set('new_user_id', response.data.user.id);",
									"        pm.environment.set('new_user_email', response.data.user.email);",
									"        console.log('New user created with API key:', response.data.user.apiKey);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"newuser@example.com\",\n  \"password\": \"NewPassword123!\",\n  \"role\": \"user\",\n  \"rateLimit\": 100\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/auth?action=create-user",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"auth"
							],
							"query": [
								{
									"key": "action",
									"value": "create-user"
								}
							]
